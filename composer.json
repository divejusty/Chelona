{
    "name": "divejusty/chelona",
    "description": "Chelona PHP Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Wies"
        }
	],
	"autoload": {
		"psr-4": {
			"Chelona\\": "src/Chelona"
        }
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
    "require": {
		"php": "^8.3",
		"ext-pdo": "*"
	},
    "require-dev": {
        "phpunit/phpunit": "^9",
        "friendsofphp/php-cs-fixer": "^3.64"
	},
	"scripts": {
		"all": [
			"composer cs",
			"composer test"
		],
		"cs": "./vendor/bin/php-cs-fixer fix src",
		"test": "./vendor/bin/phpunit tests"
	}
}
